{
    "name": "strictus/strictus",
    "description": "Strict Internal Typing For PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Miller",
            "email": "chris@jump24.co.uk",
            "role": "Developer"
        },
        {
            "name": "Wendell Adriel",
            "email": "me@wendelladriel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Strictus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strictus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.4",
        "pestphp/pest": "^1.22"
    }
}
