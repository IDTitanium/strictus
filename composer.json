{
    "name": "strictus/strictus",
    "description": "Strict Internal Typing For PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Miller",
            "email": "chris@jump24.co.uk",
            "role": "Developer"
        },
        {
            "name": "Wendell Adriel",
            "email": "chris@jump24.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Strictus\\Strictus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strictus\\Strictus\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
